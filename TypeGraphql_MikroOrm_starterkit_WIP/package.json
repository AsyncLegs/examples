{
  "name": "@goodplant/backend",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "preinit": "node -r ./register.paths.js -r dotenv-safe/config",
    "orm": "yarn preinit ./node_modules/.bin/mikro-orm",
    "build": "yarn clean && yarn install && ./node_modules/.bin/tsc --inlineSourceMap true && yarn gen-env",
    "clean": "rm -rf {dist,*.tsbuildinfo,node_modules}",
    "start": "yarn preinit ./dist/index.js",
    "dev": "tsc-watch --project ./tsconfig.dev.json --onSuccess 'yarn start' --onFailure 'echo failed to compile the sources!'",
    "migrations:create": "yarn orm migration:create",
    "migrations:up": "yarn orm migration:up",
    "migrations:down": "yarn orm migration:down",
    "gen-env": "gen-env-types .env -o env.d.ts"
  },
  "dependencies": {
    "@mikro-orm/cli": "4.2.3",
    "@mikro-orm/core": "4.2.3",
    "@mikro-orm/migrations": "4.2.3",
    "@mikro-orm/postgresql": "4.2.3",
    "@mikro-orm/reflection": "4.2.3",
    "apollo-server": "2.18.2",
    "argon2": "0.27.0",
    "class-validator": "0.12.2",
    "dotenv-safe": "8.2.0",
    "gen-env-types": "1.0.4",
    "graphql": "15.4.0",
    "jose": "2.0.3",
    "lodash": "4.17.20",
    "pg": "^8.4.2",
    "pino": "6.7.0",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.0",
    "typedi": "0.8.0",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.162",
    "@types/mongodb": "3.5.31",
    "@types/node": "14.14.5",
    "@types/pino": "6.3.3",
    "@types/uuid": "8.3.0",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^4.0.0",
    "ts-node": "^9.0.0",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/core/database/config.ts",
      "./dist/core/database/config.js"
    ]
  }
}
